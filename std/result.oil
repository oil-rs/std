use std/rt as rt
use std/unreachable for unreachable

pub enum Result {
  Ok(value: dyn),
  Err(error: dyn)
}

pub fn unwrap(result: Result): dyn {
  match result {
    Result.Ok { value } -> {
      return value
    }
    Result.Err { } -> {
      rt.panic("unwrap on error result.")
      return unreachable
    }
  }
}
